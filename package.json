    "js-yaml": "^4.1.0",
    "prettier": "^2.6.0",
    "prettier-eslint": "^15.0.0",
    "prettier-plugin-svelte": "^2.6.0",
    "sapper": "0.29.3",
    "start-server-and-test": "^1.14.0",
    "svelte": "3.47.x"
