{
  "name": "stunning-theme",
  "description": "Static Site/Blog built with Sapper with Markdown as content source",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "prettify": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --check **/*.{js,svelte,html}",
    "prettify:fix": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write **/*.{js,svelte,html}"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "gatsby-source-rss-feed": "^1.1.1",
    "polka": "next",
    "reading-time": "^1.2.0",
    "sapper-sources": "^0.0.2",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "husky": "^4.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.1.3",
    "sapper": "^0.27.11",
    "sass": "^1.24.3",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern **/*.{js,svelte,css,html}"
    }
  }
}
