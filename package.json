{
  "name": "blogmops",
  "description": "read some cool stories about a cute pug",
  "version": "0.6.0",
  "homepage": "https://blogmops.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsbase/blogmops.git"
  },
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "start": "node __sapper__/build",
    "export": "sapper export --legacy",
    "static": "npx serve __sapper__/export",
    "cy:open": "cypress open",
    "cy:run": "cypress run --record --group 2x-chrome --parallel --browser chrome",
    "cy:run:dev": "cypress run --group 2x-chrome --parallel --browser chrome",
    "start:ci": "run-s build start",
    "static:ci": "run-s export static",
    "test": "run-p -r dev cy:run",
    "test:dev": "run-p -r dev cy:run:dev",
    "test:export": "run-p -r static:ci cy:run",
    "snyk-protect": "snyk protect -prune-repeated-subdependencies",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "date-fns": "^2.15.0",
    "gray-matter": "^4.0.2",
    "helmet": "4.1.0",
    "marked": "^1.1.1",
    "polka": "^1.0.0-next.11",
    "prismjs": "^1.21.0",
    "reading-time": "^1.2.0",
    "rollup-plugin-glob": "^1.0.2",
    "sirv": "^1.0.6",
    "snyk": "^1.377.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "@cypress/code-coverage": "^3.8.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/node": "^14.6.0",
    "autoprefixer": "^9.8.6",
    "babel-plugin-istanbul": "^6.0.0",
    "cssnano": "^4.1.10",
    "cypress": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint": "7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "image-size": "^0.8.3",
    "npm-run-all": "^4.1.5",
    "os": "^0.1.1",
    "rollup": "2.26.3",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "stylelint": "^13.6.1",
    "svelte": "^3.24.1",
    "volleyball": "^1.5.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "snyk": true
}
