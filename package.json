{
  "name": "blogmops",
  "description": "A blog running with sapper.",
  "version": "0.8.1",
  "license": "MIT",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "prettify": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --check **/*.{js,svelte,html}",
    "prettify:fix": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write **/*.{js,svelte,html}"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "compression": "^1.7.x",
    "polka": "next",
    "reading-time": "^1.2.x",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "rollup": "^2.70.x",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.1",
    "sass": "^1.24.3",
    "sapper": "^0.29.3",
    "sapper-sources": "^0.0.2",
    "sirv": "^0.4.x",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.3.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^2.6.0",
    "yarn-deduplicate": "^3.1.x"
  }
}
