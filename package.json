{
  "name": "blogmops",
  "description": "read some cool stories about a cute pug",
  "version": "0.5.5",
  "homepage": "https://blogmops.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsbase/blogmops.git"
  },
  "scripts": {
    "dev": "nodenv --env .env --exec \"sapper dev\"",
    "build": "sapper build --legacy",
    "start": "nodenv --env .env __sapper__/build",
    "export": "sapper export --legacy",
    "static": "npx serve __sapper__/export",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "run-p -r export cy:run",
    "test:dev": "run-p -r build cy:run"
  },
