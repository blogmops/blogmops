{
  "name": "blogmops",
  "description": "A blog for a pug.",
  "version": "0.8.1",
  "license": "MIT",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "prettify": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --check **/*.{js,svelte,html}",
    "prettify:fix": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write **/*.{js,svelte,html}"
  },
  "dependencies": {
    "@babel/core": "7.0.x",
    "@babel/plugin-syntax-dynamic-import": "7.0.x",
    "@babel/plugin-transform-runtime": "7.0.x",
    "@babel/preset-env": "7.0.x",
    "@babel/runtime": "7.17.x",
    "@rollup/plugin-commonjs": "11.0.x",
    "@rollup/plugin-json": "4.0.x",
    "@rollup/plugin-node-resolve": "7.0.x",
    "@rollup/plugin-replace": "2.2.x",
    "compression": "1.7.x",
    "polka": "next",
    "reading-time": "1.2.x",
    "rollup": "2.70.x",
    "rollup-plugin-babel": "4.0.x",
    "rollup-plugin-sass": "1.2.x",
    "rollup-plugin-svelte": "5.0.x",
    "rollup-plugin-terser": "5.3.1",
    "sapper-sources": "0.0.2",
    "sass": "1.49.x",
    "sirv": "0.4.x",
    "svelte-preprocess": "3.3.x"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "cypress": "^9.5.2",
    "npm-run-all": "4.1.x",
    "prettier": "1.19.x",
    "prettier-plugin-svelte": "0.7.x",
    "sapper": "0.29.3",
    "svelte": "3.0.x"
  }
}
