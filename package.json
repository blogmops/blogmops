    "@babel/runtime": "7.18.x",
    "@rollup/plugin-commonjs": "21.1.x",
    "@rollup/plugin-json": "4.1.x",
    "@rollup/plugin-node-resolve": "13.3.x",
    "@rollup/plugin-replace": "2.2.x",
    "compression": "1.7.x",
    "polka": "next",
    "reading-time": "1.3.0",
    "rollup": "2.74.x",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-sass": "1.2.x",
    "rollup-plugin-svelte": "6.1.x",
