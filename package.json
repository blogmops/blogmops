{
	"name": "blogmops",
	"description": "A blog for a pug.",
	"version": "0.8.1",
	"license": "MIT",
	"scripts": {
		"dev": "sapper dev",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy",
		"start": "node __sapper__/build",
		"cy:open": "cypress open",
		"cy:dev:chromium": "cross-env NODE_ENV=development cypress run --browser=chromium --ci-build-id=githubActions --record --parallel --config-file=./cypress.env.json",
    "cy:ci:chromium": "cross-env NODE_ENV=production PORT=5000 cypress run --browser=chromium --ci-build-id=githubActions --record --parallel --config-file=./cypress.json",
		"cy:dev:browser": "start-server-and-test dev http://localhost:3000 cy:dev:chromium",
    "cy:ci:browser": "start-server-and-test build http://localhost:5000 cy:ci:chromium"
	},
	"dependencies": {
		"@babel/core": "7.0.x",
		"@babel/plugin-syntax-dynamic-import": "7.0.x",
		"@babel/plugin-transform-runtime": "7.0.x",
		"@babel/preset-env": "7.0.x",
		"@babel/runtime": "7.17.x",
		"@rollup/plugin-commonjs": "11.0.x",
		"@rollup/plugin-json": "4.0.x",
		"@rollup/plugin-node-resolve": "7.0.x",
		"@rollup/plugin-replace": "2.2.x",
		"compression": "1.7.x",
		"polka": "next",
		"reading-time": "1.2.1",
		"rollup": "2.70.x",
		"rollup-plugin-babel": "4.4.0",
		"rollup-plugin-sass": "1.2.x",
		"rollup-plugin-svelte": "5.0.x",
		"rollup-plugin-terser": "5.3.1",
		"sapper-sources": "0.0.2",
		"sass": "1.49.x",
		"sirv": "0.4.x",
		"svelte-preprocess": "^3.9.12"
	},
	"devDependencies": {
		"@cypress/code-coverage": "^3.9.12",
		"cross-env": "7.0.3",
		"cypress": "^9.5.2",
		"dayjs": "1.10.4",
		"eslint": "^8.11.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-prettier": "4.0.0",
		"js-yaml": "^3.14.1",
		"npm-run-all": "4.1.x",
		"prettier": "^2.6.0",
		"prettier-eslint": "^13.0.0",
		"prettier-plugin-svelte": "^2.6.0",
		"sapper": "0.29.3",
		"start-server-and-test": "^1.14.0",
		"svelte": "3.0.x"
	}
}
