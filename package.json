{
    "name": "blogmops",
    "description": "A blog for a pug.",
    "version": "0.8.2",
    "license": "MIT",
    "scripts": {
        "dev": "sapper dev",
        "build": "sapper build --legacy",
        "export": "sapper export --legacy",
        "start": "node __sapper__/build",
        "cy:open": "cypress open",
        "cy:run": "cypress run --config-file=cypress.env.json --env={host=localhost,port=3000}",
        "cy:chromium": "yarn run cy:run --browser chromium --headed",
        "cy:firefox": "yarn run cy:run --browser firefox --headed",
        "update:browserslist": "browserslist --coverage=DE,global --mobile-to-desktop --ignore-unknown-versions && browserslist --update-db"
    },
    "dependencies": {
        "@babel/core": "7.17.9",
        "@babel/plugin-syntax-dynamic-import": "7.8.3",
        "@babel/plugin-transform-runtime": "7.17.0",
        "@babel/preset-env": "7.16.11",
        "@babel/runtime": "7.17.x",
        "@rollup/plugin-commonjs": "21.1.0",
        "@rollup/plugin-json": "4.1.0",
        "@rollup/plugin-node-resolve": "13.2.1",
        "@rollup/plugin-replace": "4.0.0",
        "compression": "1.7.x",
        "polka": "next",
        "reading-time": "1.3.0",
        "rollup": "2.70.x",
        "rollup-plugin-babel": "4.4.0",
        "rollup-plugin-sass": "1.2.x",
        "rollup-plugin-svelte": "6.1.1",
        "rollup-plugin-terser": "7.0.2",
        "sapper-sources": "0.0.2",
        "sass": "1.51.x",
        "sirv": "2.0.x",
        "svelte-preprocess": "4.10.6"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.9.12",
        "browserslist": "^4.20.2",
        "cross-env": "7.0.3",
        "cypress": "^9.5.2",
        "dayjs": "1.11.1",
        "eslint": "^8.11.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-prettier": "4.0.0",
        "js-yaml": "^4.1.0",
        "prettier": "^2.6.0",
        "prettier-eslint": "^13.0.0",
        "prettier-plugin-svelte": "^2.6.0",
        "sapper": "0.29.3",
        "start-server-and-test": "^1.14.0",
        "svelte": "3.47.x"
    }
}
